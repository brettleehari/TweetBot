{
  "name": "tweetbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "test:collector": "jest src/mastra/agents/data-collector-agent.test.ts --runInBand --detectOpenHandles",
    "test:quick": "npm run test:collector",
    "dev": "npm run test:quick && mastra dev",
    "build": "tsc && npm run build-web",
    "build-web": "mkdir -p dist && cp -r docs/* dist/ 2>/dev/null || true",
    "start": "node dist/enhanced-24x7-trading.js",
    "start:dev": "node --loader ts-node/esm enhanced-24x7-trading.ts",
    "trading": "node --loader ts-node/esm enhanced-24x7-trading.ts",
    "trading-prod": "NODE_ENV=production npm run build && npm start",
    "server": "node --loader ts-node/esm src/server.ts",
    "test-content": "node --loader ts-node/esm src/mastra/testing/test-content-generation.ts",
    "test-content-full": "npm run test-content full",
    "test-content-quick": "npm run test-content quick",
    "test-agentic": "node --loader ts-node/esm src/mastra/testing/agent-evaluation.ts",
    "test-agent-bench": "node --loader ts-node/esm src/mastra/testing/agent-test-bench.ts",
    "demo-agents": "node --loader ts-node/esm src/mastra/testing/run-agent-demo.ts",
    "demo-agents-simple": "node --loader ts-node/esm src/mastra/testing/simple-agent-demo.ts",
    "test-database": "node --loader ts-node/esm src/mastra/testing/test-database.ts",
    "demo-core-agency": "node --loader ts-node/esm src/mastra/testing/core-agency-demo.ts",
    "agent-cli": "node --loader ts-node/esm src/mastra/testing/agent-cli.ts",
    "test-performance-optimizer": "node --loader ts-node/esm src/mastra/testing/test-performance-optimizer.ts",
    "live-stream": "node --loader ts-node/esm src/mastra/testing/simple-local-streaming.ts",
    "stream-demo": "node --loader ts-node/esm src/mastra/testing/simple-streaming-demo.ts",
    "web-server": "node --loader ts-node/esm src/mastra/testing/local-web-server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@mastra/core": "^0.14.1",
    "@mastra/libsql": "^0.13.4",
    "@mastra/loggers": "^0.10.7",
    "@mastra/memory": "^0.13.1",
    "@types/node-fetch": "^2.6.13",
    "@types/ws": "^8.18.1",
    "ai": "^4.3.19",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.15.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "twitter-api-v2": "^1.25.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/sqlite3": "^5.1.0",
    "jest": "^30.0.5",
    "mastra": "^0.10.23",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
